---
import HeadMeta from '../components/HeadMeta.astro';
let title = 'More UI';
let description = 'More usable interfaces in components';
---

<!DOCTYPE html>
<html lang="en">
<head>
    <HeadMeta title={title} description={description} />
</head>
<body>
    <div class="bg-green-100 rounded-lg p-4 m-4 text-sm text-green-700">
        <h1>{title}</h1>
        <p class="font-medium">{description}</p>
    </div>
</body>
</html>